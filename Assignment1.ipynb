   "source": [
    "install.packages('rtools')\n",
    " install.packages('openxlsx')\n",
    " install.packages('ggplot2')\n",
    " install.packages('data.table')\n",
    " install.packages('skimr')\n",
    " install.packages('GGally')\n",
    " install.packages('ggcorrplot')\n",
    " install.packages('forecast')\n",
    " install.packages(\"xlsx\")\n",
    " \n",
    " library(openxlsx)\n",
    " library(xlsx)\n",
    "\n",
    "require(openxlsx) \n",
    "require(ggplot2)\n",
    "require(data.table)\n",
    "require(skimr)\n",
    "require(GGally)\n",
    "require(ggcorrplot)\n",
    "require(forecast)\n",
    " \n",
    "# C:\\Users\\Vusal Ibrahimli\\OneDrive\\Desktop\\EVDS2.xlsx\n",
    "#Are enflation rate, eletricity production or industry ratio related to electric, consumer price index and industry words that people search in google?"
    " data_path <- \"C:/Users/Vusal Ibrahimli/OneDrive/Desktop/EVDS3.xlsx\"\n",
    " EVDS=read.xlsx(data_path,sheet='data')\n",
    " EVDSdate=read.xlsx(data_path,sheet='data_wdate')\n",
    " str(EVDSdate)\n",
    " str(EVDS)\n",
    " EVDSdate$Date <- as.Date(EVDSdate$Date, format = \"%m-%Y\")\n",
    " \n",
    " start_date <- as.Date(\"2019-01-01\")\n",
    " end_date <- as.Date(\"2023-12-01\")\n",
    " dates_sequence <- seq.Date(start_date, end_date, by = \"month\")\n",
    " \n",
    " \n",
    " summary_data=skim(EVDSdate)\n",
    " print(summary_data)\n",
    "ggpairs(EVDS)\n",
    "\n",
    "#EVDS datalari\n",
    "\n",
    "ggplot(EVDS, aes(x=ELEKTRIK.URETIMI)) + geom_histogram()\n",
    "ggplot(EVDS, aes(x=FIYAT.ENDEKSI)) + geom_histogram()\n",
    "ggplot(EVDS, aes(x=IMALAT.SANAYI.ORANI)) + geom_histogram()\n",
    "\n",
    "\n",
    "ggplot(EVDSdate, aes(x=ELEKTRIK.URETIMI)) + geom_boxplot() \n",
    "ggplot(EVDSdate, aes(x=FIYAT.ENDEKSI)) + geom_boxplot()\n",
    "ggplot(EVDSdate, aes(x=IMALAT.SANAYI.ORANI)) + geom_boxplot()\n",
    "\n",
    "ggplot(EVDSdate, aes(y=ELEKTRIK.URETIMI, x= Date)) + geom_line() #seasonality\n",
    "ggplot(EVDSdate, aes(y=FIYAT.ENDEKSI, x= Date)) + geom_line() #exponentially/trend\n",
    "ggplot(EVDSdate, aes(y=IMALAT.SANAYI.ORANI, x= Date)) + geom_line()\n",
    "\n",
    "ggplot(EVDSdate, aes(y=IMALAT.SANAYI.ORANI, x= Date)) + geom_point()#seasonality and outliers\n",
    "ggplot(EVDSdate, aes(y=FIYAT.ENDEKSI, x= Date)) + geom_point()\n",
    "ggplot(EVDSdate, aes(y=ELEKTRIK.URETIMI, x= Date)) + geom_point() #outliers\n",
    "\n",
    "#Google Trends datalari\n",
    "ggplot(EVDSdate, aes(y=Enflasyon, x= Date)) + geom_line() #trend\n",
    "ggplot(EVDSdate, aes(y=Elektrik, x= Date)) + geom_line()\n",
    "ggplot(EVDSdate, aes(y=Sanayi.Üretimi , x= Date)) + geom_line()\n",
    "\n",
    "ggplot(EVDSdate, aes(y=Enflasyon, x= Date)) + geom_point()\n",
    "ggplot(EVDSdate, aes(y=Elektrik, x= Date)) + geom_point() #seasonality\n",
    "ggplot(EVDSdate, aes(y=Sanayi.Üretimi , x= Date)) + geom_point()\n",
    "\n",
    "ggplot(EVDS, aes(x=Enflasyon)) + geom_histogram()\n",
    "ggplot(EVDS, aes(x=Elektrik)) + geom_histogram()\n",
    "ggplot(EVDSdate, aes(x=Sanayi.Üretimi )) + geom_histogram()\n",
    "\n",
    "#karsilastirma\n",
    "\n",
    "ggplot(EVDSdate, aes(y=Enflasyon, x=FIYAT.ENDEKSI )) + geom_point()+geom_smooth()\n",
    "ggplot(EVDSdate, aes(y=Elektrik, x=ELEKTRIK.URETIMI )) + geom_point()+geom_smooth()\n",
    "ggplot(EVDSdate, aes(y=Sanayi.Üretimi, x=IMALAT.SANAYI.ORANI )) + geom_point()+geom_smooth()\n",
    "\n",
    "\n",
    "EVDSdate$mon <- format(EVDSdate$Date, \"%B\")\n",
    "  # %B ile ay isimlerini alıyoruz\n",
    "head(EVDSdate)\n",
    "ggplot(EVDSdate,aes(y=Enflasyon, x=FIYAT.ENDEKSI)) +\n",
    " geom_point() + geom_smooth(method=lm) + facet_wrap(~mon,scales='free')\n",
    "ggplot(EVDSdate,aes(y=Elektrik, x=ELEKTRIK.URETIMI)) +\n",
    "  geom_point() + geom_smooth(method=lm) + facet_wrap(~mon,scales='free')\n",
    "ggplot(EVDSdate,aes(y=Sanayi.Üretimi, x=IMALAT.SANAYI.ORANI)) +\n",
    "  geom_point() + geom_smooth(method=lm) + facet_wrap(~mon,scales='free')\n",
    "\n",
    "\n",
    "#Time Series Regression models\n",
    "\n",
    "l_fit_fi = lm(FIYAT.ENDEKSI~ Enflasyon + Elektrik + Sanayi.Üretimi ,data=EVDS)\n",
    "l_fit_fi\n",
    "summary(l_fit_fi)\n",
    "plot(l_fit_fi)\n",
    "checkresiduals(l_fit_fi) # residuals is normal distribution\n",
    "ggplot(EVDS$t,EVDS$FIYAT.ENDEKSI) # exponentionally\n",
    "CV(l_fit_fi)\n",
    "\n",
    "\n",
    "l_fit_fi_log = lm(log(FIYAT.ENDEKSI) ~ EVDS$t+(Enflasyon) + (Elektrik) + (Sanayi.Üretimi) ,data=EVDS)\n",
    "l_fit_fi_log\n",
    "summary(l_fit_fi_log)\n",
    "plot(l_fit_fi_log)\n",
    "checkresiduals(l_fit_fi_log)\n",
    "CV(l_fit_fi_log)\n",
    "\n",
    "\n",
    "EVDS$fitted <- fitted(l_fit_fi_log)\n",
    "EVDS$residual <- residuals(l_fit_fi_log)\n",
    "\n",
    "EVDS %>%\n",
    "  ggplot(aes(x=fitted,y=log(FIYAT.ENDEKSI))) +\n",
    "  geom_point() + \n",
    "  geom_smooth(method = lm)\n",
    "\n",
    "l_fit_eu = lm(ELEKTRIK.URETIMI~ Enflasyon + Elektrik + Sanayi.Üretimi ,data=EVDS)\n",
    "l_fit_eu\n",
    "summary(l_fit_eu)\n",
    "plot(l_fit_eu)\n",
    "checkresiduals(l_fit_eu)\n",
    "plot(EVDSdate$Date,EVDS$ELEKTRIK.URETIMI)#polynomial\n",
    "\n",
    "l_fit_eu_poly = lm(log(ELEKTRIK.URETIMI)~ (EVDSdate$mon+ Enflasyon + Elektrik + Sanayi.Üretimi) ,data=EVDS)\n",
    "l_fit_eu_poly\n",
    "summary(l_fit_eu_poly)\n",
    "plot(l_fit_eu_poly)\n",
    "checkresiduals(l_fit_eu_poly)\n",
    "EVDS$fitted1 <- fitted(l_fit_eu_poly)\n",
    "EVDS %>%\n",
    "  ggplot(aes(x=fitted,y=log(ELEKTRIK.URETIMI))) +\n",
    "  geom_point() + \n",
    "  geom_smooth(method = lm)\n",
    "\n",
    "\n",
    "l_fit_iso = lm(IMALAT.SANAYI.ORANI~ Enflasyon + Elektrik + Sanayi.Üretimi ,data=EVDS)\n",
    "l_fit_iso\n",
    "summary(l_fit_iso)\n",
    "plot(l_fit_iso)\n",
    "checkresiduals(l_fit_iso)\n",
    "plot(EVDSdate$Date,EVDS$IMALAT.SANAYI.ORANI)#polynomial\n",
    "\n",
    "l_fit_iso_log = lm(poly(IMALAT.SANAYI.ORANI) ~ EVDSdate$mon_t+(Enflasyon) + (Elektrik) + (Sanayi.Üretimi) ,data=EVDS)\n",
    "l_fit_iso_log\n",
    "summary(l_fit_iso_log)\n",
    "plot(l_fit_iso_log)\n",
    "checkresiduals(l_fit_iso_log)\n",
    "\n",
    "library(data.table)\n",
    "\n",
    "# Veri tablosu oluşturma veya kopyalama\n",
    "\n",
    "EVDS2$Tuketici.Fiyat.Endeksi_kare <- EVDS2$Tuketici.Fiyat.Endeksi^2\n",
    "\n",
    "# Yeni model oluştur\n",
    "l_fit_quadratic <- lm(Tuketici.Fiyat.Endeksi ~ ., data = EVDS2)\n",
    "summary(l_fit_quadratic)\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "EVDS2$log_Tuketici <- log(EVDS2$Tuketici.Fiyat.Endeksi)\n",
    "\n",
    "# Yeni model oluştur\n",
    "l_fit_log <- lm(log_Tuketici ~ ., data = EVDS2)\n",
    "summary(l_fit_log)\n",
    "plot(l_fit_log)\n",
    "checkresiduals(l_fit_poly)\n",
    "CV(l_fit_poly)\n",
    "\n",
    "EVDS$poly_Tuketici <- poly(EVDS2$Tuketici.Fiyat.Endeksi)\n",
    "\n",
    "# Yeni model oluştur\n",
    "l_fit_poly <- lm(poly_Tuketici ~ ., data = EVDS)\n",
    "summary(l_fit_poly)\n",
    "plot(l_fit_poly)"
   ]
  }
